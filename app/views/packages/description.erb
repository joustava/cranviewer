<h2><%= @package.name %></h2>

<ul>
<% for package in @package.versions %>
  <li>
    <p>
      <%= package.description.attributes %>
    </p>
    <p>
    version <%= package.version %>, published <%= package.description.publication %></p>
    </p>
    <p>
      <h3>Title</h3>
      <%= package.description.title %>
    </p>
    <p>
      <h3>Author(s)</h3>
      <%= package.description.author %>
    </p>
    <p>
      <h3>Maintainer(s)</h3>
      <%= package.description.maintainer %>
    </p>
    <p>
      <h3>Description</h3>
      <%= package.description.description %>
    </p>
  </li>
<% end %>
</ul>
